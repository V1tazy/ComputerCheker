<Application x:Class="ComputerCheker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ComputerCheker"
             xmlns:vm ="clr-namespace:ComputerCheker.ViewModels"
             StartupUri="\Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Main.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <vm:ViewModelLocator x:Key="Locator"></vm:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
